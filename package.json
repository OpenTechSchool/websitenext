{
  "name": "ots_website",
  "version": "2.0.0",
  "description": "OpenTechSchool website 2.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .tsx,.js pages/**/** components/**/**",
    "lint:watch": "nodemon --exec \"npm run lint || exit 1\" --ext ts,js",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "prod": "next start",
    "open": "sleepms 5000 && open http://localhost:3000",
    "dev": "npm run clean-cache && npm run lint && concurrently \"npm run lint:watch\" next \"npm run open\" --names \"üõÇ,üèóÔ∏è,üñ•Ô∏è\" --prefix name ",
    "build": "npm run clean-cache && next build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "ots",
    "next"
  ],
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "date-fns": "^2.12.0",
    "fetch-jsonp": "^1.1.3",
    "gray-matter": "^4.0.2",
    "next": "^9.3.5",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^5.0.0",
    "react-twitter-embed": "^3.0.3",
    "styled-components": "^4.4.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@material-ui/icons": "^4.9.1",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "lodash": "^4.17.19",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "sleep-ms": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false
  },
  "snyk": true
}
