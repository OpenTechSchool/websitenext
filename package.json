{
  "name": "ots_website",
  "version": "2.0.0",
  "description": "OpenTechSchool website 2.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .tsx,.js pages/**/** components/**/**",
    "lint:watch": "nodemon --exec \"npm run lint || exit 1\" --ext ts,js",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "prod": "next start",
    "open": "sleepms 5000 && open http://localhost:3000",
    "dev": "npm run clean-cache && npm run lint && concurrently \"npm run lint:watch\" next \"npm run open\" --names \"üõÇ,üèóÔ∏è,üñ•Ô∏è\" --prefix name ",
    "build": "npm run clean-cache && next build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "ots",
    "next"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "date-fns": "^2.17.0",
    "fetch-jsonp": "^1.1.3",
    "gray-matter": "^4.0.2",
    "next": "^10.0.7",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-twitter-embed": "^3.0.3",
    "styled-components": "^5.2.1",
    "snyk": "^1.518.0"
  },
  "devDependencies": {
    "@material-ui/icons": "^4.11.2",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "sleep-ms": "^2.0.1",
    "typescript": "^4.2.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false
  },
  "snyk": true
}
